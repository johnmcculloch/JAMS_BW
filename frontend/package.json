{
  "name": "frontend",
  "version": "1.0.0",
  "description": "A React application",
  "main": "main.js",
  "scripts": {
    "start": "parcel public/index.html --open",
    "build": "parcel build public/index.html --out-dir dist --public-url ./",
    "electron-dev": "concurrently \"npm:start\" \"wait-on http://localhost:1234 && electron .\"",
    "electron": "npm run build && electron .",
    "dist": "PYTHON=/usr/bin/python3 npm run build && electron-builder"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.0",
    "@mui/material": "^6.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "electron": "^33.3.1",
    "electron-builder": "^25.1.8",
    "electron-is-dev": "^2.0.0",
    "parcel-bundler": "^1.12.5",
    "wait-on": "^5.3.0"
  },
  "build": {
    "appId": "com.example.react-electron-app",
    "files": [
      "dist/**/*",
      "main.js",
      "preload.js",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
